{
  "name": "medemo",
  "version": "1.0.0",
  "description": "Emergency Healthcare Response App - Production Ready",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "healthcare",
    "emergency",
    "ambulance",
    "hospital",
    "blood-bank",
    "telemedicine"
  ],
  "author": "MedEmo Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "socket.io": "^4.7.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "twilio": "^4.12.0",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1",
    "geoip-lite": "^1.4.7",
    "node-cron": "^3.0.2",
    "sharp": "^0.32.5",
    "cloudinary": "^1.40.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "eslint": "^8.47.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
